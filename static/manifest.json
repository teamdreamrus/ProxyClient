{
  "version": "1.0.0.0",
  "name": "Free Proxy",
  "description": "Free proxy or use your proxy in our client",
  "default_locale": "ru",
  "author": "AlexeyTop1",
  "manifest_version": 2,
  "permissions": [
    "notifications",
    "storage",
    "tabs",
    "cookies",
    "<all_urls>",
    "proxy",
    "webRequest",
    "webRequestBlocking"
  ],
  "background": {
    "scripts": ["vendors/bundle.js", "bg/bundle.js"],
    "persistent": true
  },
  "browser_action": {
    "default_icon": "images/256x256.png",
    "default_title": "FreeProxyClient",
    "default_popup": "popup.html"
  },
  "icons": {
    "48": "images/256x256.png"
  }
   ,
  "web_accessible_resources": [
    "popup/*css",
    "*.css",
    "images",
    "*.png"
  ]
}
